<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group_3"
                android:pivotX="12"
                android:pivotY="12">
                <group
                    android:name="group_1"
                    android:pivotX="12"
                    android:pivotY="12">
                    <group
                        android:name="group_2"
                        android:pivotX="12"
                        android:pivotY="12">
                        <path
                            android:name="path"
                            android:pathData="M 3 12 L 21 12"
                            android:strokeColor="#000000"
                            android:strokeAlpha="0"
                            android:strokeWidth="2"/>
                    </group>
                    <path
                        android:name="path843"
                        android:pathData="M 3 12 L 21 12"
                        android:strokeColor="#000000"
                        android:strokeWidth="2"
                        android:trimPathEnd="0.6666666666666666"
                        android:fillType="evenOdd"/>
                </group>
                <group android:name="group">
                    <path
                        android:name="path817_1"
                        android:pathData="M 3 17 L 21 17"
                        android:strokeColor="#000000"
                        android:strokeWidth="2"
                        android:trimPathEnd="0.3333333333333333"
                        android:fillType="evenOdd"/>
                    <path
                        android:name="path843_9"
                        android:pathData="M 3 7 L 21 7"
                        android:strokeColor="#000000"
                        android:strokeWidth="2"
                        android:fillType="evenOdd"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="path843_9">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathStart"
                android:duration="250"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="path817_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathStart"
                android:duration="179"
                android:valueFrom="0"
                android:valueTo="0.3333333333333333"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="path843">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="trimPathEnd"
                android:duration="179"
                android:valueFrom="0.6666666666666666"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="group_1">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="189"
                android:valueFrom="0"
                android:valueTo="45"
                android:valueType="floatType"
                android:interpolator="@android:anim/accelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:startOffset="179"
                android:duration="10"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="group_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:startOffset="189"
                android:duration="191"
                android:valueFrom="0"
                android:valueTo="90"
                android:valueType="floatType"
                android:interpolator="@android:anim/decelerate_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group_3">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="rotation"
                android:duration="380"
                android:valueFrom="0"
                android:valueTo="180"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
