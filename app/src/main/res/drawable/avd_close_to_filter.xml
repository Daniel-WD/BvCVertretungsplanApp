<!-- Animation von SchlieÃŸen-Icon zu Filter-Icon -->
<animated-vector
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt">
    <aapt:attr name="android:drawable">
        <vector
            android:name="vector"
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <group
                android:name="group"
                android:pivotX="12"
                android:pivotY="12">
                <group
                    android:name="group_5"
                    android:pivotX="12"
                    android:pivotY="7"
                    android:translateY="5"
                    android:rotation="-45">
                    <path
                        android:name="path_5"
                        android:pathData="M 3 7 L 21 7"
                        android:strokeColor="#000000"
                        android:strokeWidth="2"/>
                </group>
                <group
                    android:name="group_2"
                    android:pivotX="12"
                    android:pivotY="7"
                    android:translateY="5"
                    android:rotation="45">
                    <path
                        android:name="path_1"
                        android:pathData="M 3 7 L 21 7"
                        android:strokeColor="#000000"
                        android:strokeWidth="2"/>
                </group>
                <group android:name="group_3">
                    <path
                        android:name="path_2"
                        android:pathData="M 6 12 L 18 12"
                        android:strokeColor="#000000"
                        android:strokeAlpha="0"
                        android:strokeWidth="2"/>
                </group>
                <group
                    android:name="group_4"
                    android:translateY="-5">
                    <path
                        android:name="path"
                        android:pathData="M 10 17 L 14 17"
                        android:strokeColor="#000000"
                        android:strokeAlpha="0"
                        android:strokeWidth="2"/>
                </group>
            </group>
        </vector>
    </aapt:attr>
    <target android:name="group_2">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="150"
                    android:valueFrom="45"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_2">
    <aapt:attr name="android:animation">
        <set>
            <objectAnimator
                android:propertyName="translateY"
                android:startOffset="150"
                android:duration="198"
                android:valueFrom="5"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:anim/overshoot_interpolator"/>
        </set>
    </aapt:attr>
</target>
    <target android:name="group_4">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="translateY"
                android:startOffset="150"
                android:duration="198"
                android:valueFrom="-5"
                android:valueTo="0"
                android:valueType="floatType"
                android:interpolator="@android:anim/overshoot_interpolator"/>
        </aapt:attr>
    </target>
    <target android:name="group_5">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="translateY"
                    android:startOffset="150"
                    android:duration="198"
                    android:valueFrom="5"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/overshoot_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="group_5">
        <aapt:attr name="android:animation">
            <set>
                <objectAnimator
                    android:propertyName="rotation"
                    android:duration="150"
                    android:valueFrom="-45"
                    android:valueTo="0"
                    android:valueType="floatType"
                    android:interpolator="@android:anim/accelerate_interpolator"/>
            </set>
        </aapt:attr>
    </target>
    <target android:name="path_2">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:startOffset="140"
                android:duration="10"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
    <target android:name="path">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:propertyName="strokeAlpha"
                android:startOffset="140"
                android:duration="10"
                android:valueFrom="0"
                android:valueTo="1"
                android:valueType="floatType"
                android:interpolator="@android:interpolator/fast_out_slow_in"/>
        </aapt:attr>
    </target>
</animated-vector>
